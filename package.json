{
  "name": "mimo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- -d ./src/data-source.ts migration:generate ./src/migration/InitialSetup",
    "migration:run": "npm run typeorm -- -d ./src/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- -d ./src/data-source.ts migration:revert",
    "db:seed": "ts-node ./src/seed.ts",
    "test": "jest",
    "start": "ts-node src/server.ts",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "serve": "node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "express": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/sqlite3": "^3.1.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2"
  }
}
